#include <stdio.h>
#include <math.h>
#define PI 3.14159

int main(){
	double A,B,C,Triangulo, Circulo, Trapezio, Quadrado, Retangulo;
	scanf("%lf %lf %lf",&A,&B,&C);	
	Triangulo=(A*C)/2.0;
	Circulo=pow(C,2)*PI;
	Trapezio=((A+B)*C)/2.0;
	Quadrado=pow(B,2);
	printf("TRIANGULO: %lf\nCIRCULO: %lf\nTRAPEZIO: %lf\n QUADRADO: %lf\n RETANGULO: %lf\n",Triangulo,Circulo,Trapezio,Quadrado,Retangulo);
	return 0;	
}
